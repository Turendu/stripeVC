{
    "name": "nectica/plataforma",
    "description": "Plataforma API de Nectica",
    "keywords": [
        "Plataforma",
        "Nectica"
    ],
    "homepage": "https://nectica.com/home",
    "license": "MIT",
    "authors": [
        {
            "name": "Nectica",
            "email": "info@nectica.com",
            "homepage": "https://nectica.com/home"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "composer/composer": "1.10.22",
        "doctrine/migrations": "^3.3",
        "firebase/php-jwt": "^5.0",
        "palanik/corsslim": "dev-slim3",
        "predis/predis": "^1.1",
        "respect/validation": "^1.1",
        "slim/slim": "^3.12.3",
        "stripe/stripe-php": "^7.108",
        "vlucas/phpdotenv": "^5.1"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^1.14",
        "phpstan/phpstan": "^0.12",
        "vimeo/psalm": "^3.15"
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 0,
        "platform": {
            "php": "7.4.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}